{
  "name": "@fangcha/app-request",
  "version": "1.6.0",
  "description": "A simple networking framework for node.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "nyc mocha --require ts-node/register tests/**/*.test.ts --timeout 100000 --exit",
    "lint": "eslint . --fix --cache",
    "build": "rm -rf ./lib/ && tsc"
  },
  "repository": "https://github.com/fangqk1991/node-request.git",
  "keywords": [
    "http",
    "request",
    "promise",
    "async"
  ],
  "author": "fang <me@fangqk.com>",
  "license": "MIT",
  "dependencies": {
    "@fangcha/app-error": "^1.5.0",
    "axios": "^1.1.3",
    "form-data": "^4.0.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@fangcha/eslint": "^0.3.2",
    "@types/assert": "^1.5.6",
    "@types/mocha": "^10.0.0",
    "@types/qs": "^6.9.1",
    "@types/request-promise": "^4.1.44",
    "@types/tunnel": "^0.0.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.1",
    "tunnel": "^0.0.6",
    "typescript": "^4.8.4"
  },
  "prettier": {
    "arrowParens": "always",
